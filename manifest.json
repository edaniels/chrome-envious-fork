{
  "manifest_version": 2,
  "name": "Chrome Envious",
  "version": "1.0",
  "description": "Run live environments against assets served from localhost",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage", "webRequestBlocking", "webRequest", "*://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": { "128": "icon-128.png" }
}
